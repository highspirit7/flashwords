    "autoprefixer": "^10.4.19",
    "chance": "^1.1.12",
    "eslint": "^8.57.0",
    "eslint-plugin-playwright": "^1.5.4",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "postcss-html": "^1.6.0",
    "prettier": "^3.3.3",
    "stylelint": "^16.3.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "tailwindcss": "^3.4.3",
